<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">
	<display-name>webstore</display-name>
	<servlet>
		<servlet-name>products</servlet-name>
		<servlet-class>com.wap.controller.ProductsServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>products</servlet-name>
		<url-pattern>/products</url-pattern>
		<url-pattern></url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>cartViewServlet</servlet-name>
		<servlet-class>com.wap.controller.CartViewServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>cartViewServlet</servlet-name>
		<url-pattern>/cartViewServlet</url-pattern>
	</servlet-mapping>
</web-app>